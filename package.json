{
	"name": "synix-ai",
	"private": true,
	"sideEffects": false,
	"type": "module",
	"scripts": {
		"build": "remix vite:build",
		"dev": "concurrently \"remix vite:dev\" \"cross-env HOST=localhost PORT=1234 npx y-websocket\" --kill-others",
		"start": "remix-serve ./build/server/index.js",
		"typecheck": "tsc",
		"lint": "biome lint . --write",
		"format": "biome format . --write"
	},
	"dependencies": {
		"@biomejs/biome": "^1.8.3",
		"@remix-run/node": "^2.10.2",
		"@remix-run/react": "^2.10.2",
		"@remix-run/serve": "^2.10.2",
		"isbot": "^4.1.0",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"tldraw": "^2.3.0",
		"y-utility": "^0.1.4",
		"y-websocket": "^2.0.3",
		"yjs": "^13.6.18",
		"zustand": "^4.5.4"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.8.3",
		"@remix-run/dev": "^2.10.2",
		"@types/react": "^18.2.20",
		"@types/react-dom": "^18.2.7",
		"autoprefixer": "^10.4.19",
		"concurrently": "^8.2.2",
		"cross-env": "^7.0.3",
		"postcss": "^8.4.38",
		"tailwindcss": "^3.4.4",
		"typescript": "^5.1.6",
		"vite": "^5.1.0",
		"vite-tsconfig-paths": "^4.2.1"
	},
	"engines": {
		"node": ">=20.0.0"
	},
	"packageManager": "yarn@4.3.1"
}
